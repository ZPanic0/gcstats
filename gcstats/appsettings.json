{
  "ArchiveSettings": {
    "FileNameTemplate": "{0}.htm",
    "ArchivePathTemplate": "{0}/pages/{1}.zip"
  },
  "ProtobufSettings": {
    "PathTemplate": "{0}/protobuf/{1}.bin"
  },
  "LodestoneUrlTemplate": "https://na.finalfantasyxiv.com/lodestone/ranking/gc/{0}/{1}/?page={2}&filter=1&gcid={3}&worldname={4}",
  "Paths": {
    "Table": "//div[@class='ranking__layout']",
    "Page": "//div[@class=\"btn__pager pager__total\"]/ul/li[@class=\"pager__total__nav\"]/a[@class=\"btn__pager--selected\"]",
    "TallyingPeriod": "//div[@class=\"ldst__window ranking__calendar\"]/p",
    "BasePath": "//tbody/tr",
    "Rank": "./td[contains(@class,\"ranking-character__number\")]",
    "PortraitUrl": "./td[contains(@class,\"ranking-character__face\")]/img",
    "PlayerName": "./td[contains(@class,\"ranking-character__info\")]/h4",
    "ServerAndDatacenterName": "./td[contains(@class,\"ranking-character__info\")]/p",
    "FactionAndRankName": "./td[contains(@class,\"ranking-character__gcrank\")]/img",
    "CompanySeals": "./td[contains(@class,\"ranking-character__value\")]"
  }
}